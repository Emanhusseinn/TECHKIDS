ent/validateLogin\"\n\n\n\n// eslint-disable-next-line \n/****************************************************************/\nconst Signin = (validate)=>{\n  const history = useHistory();\n  //the values wich inserted by the user is stored at email and password using setEmail,setPasswword \n  const [ email        , setEmail    ]     = useState();\n  const [ password     , setPassword ]     = useState();\n  const [ errors     , setErrors ]     = useState({email:\"\",password:\"\"});\n\n\n   \n function validate(email,password) {\n  let errors = {};\n  if (!email) {\n    errors.email = \"Email address is required\";\n\n  } else if (!/\\S+@\\S+\\.\\S+/.test(email)) {\n    errors.email = \"Email address is invalid\";\n  }\n  if (!password) {\n    errors.password = \"Password is required\";\n  } else if (password.length < 10) {\n    errors.password = \"Password needs to be more than 10 characters\";\n  }\n  return errors;\n}\n  \n  //we need to send the data from frontend to backend , I will use axios for that ..\n  const submit =async (e)=>{\n    e.preventDefault();\n    setErrors(validate(email,password))\n  \n\n    try {\n      const newUser = { email ,password } ;\n      const loginRes = await axios.post(\"http://localhost:8000/api/login\" , newUser)\n      console.log(loginRes.data.token)\n      localStorage.setItem(\"theToken\", loginRes.data.token);\n      history.push('/')\n       } catch (error) {\n      // alert(error.response.data.msg)\n\n  \n      }\n\n   \n    }\n\n   \n\n  return(\n  <div className=\"container p-5\" size=\"6\" className=\"py-3 px-md-5\" style={{marginTop: 10 + 'em'}}>\n  <form>    \n       \n <div className=\"form-group\" class=\"text-center\">\n <label htmlFor=\"formGroupExampleInput\">\n        <InputGroup className=\"mb-3\">\n        <InputGroup.Append>\n        <InputGroup.Text id=\"basic-addon2\">@example.com</InputGroup.Text>\n        </InputGroup.Append>\n        <FormControl\n        name\n          placeholder=\"Recipient's username\"\n          aria-label=\"Recipient's username\"\n          aria-describedby=\"basic-addon2\"\n          type=\"text\" className=\"form-control\" placeholder=\"Enter Your email\"\n          onChange={(e)=>{setEmail(e.target.value)}} \n          required\n        />\n        </InputGroup> \n       <p>{errors.email &&<p>{errors.email}</p>}</p> \n  \n    <div className=\"form-group\">\n        <InputGroup className=\"mb-3\">\n        <FormControl\n          placeholder=\"Recipient's password\"\n          aria-label=\"Recipient's password\"\n          aria-describedby=\"basic-addon2\"\n          type=\"password\" className=\"form-control\"\n          placeholder=\"Enter Your Password\" \n           onChange={(e)=>{setPassword(e.target.value)}}\n            required\n         />\n        </InputGroup> \n        <p>{errors.password &&<p>{errors.password}</p>}</p> \n    </div>\n        <div>\n           \n            <Button as=\"input\" className=\"btn btn-primary\" type=\"submit\" value=\"Submit\"  onClick={submit}    />{' '}\n        </div>\n</label>   \n</div>\n   \n       \n    </form> \n \n \n  </div>\n  \n  \n)};\n\nexport default Signin;\n\n","/Users/rbk02/Desktop/teck-kids/frontend/src/component/registrate.js",[],"/Users/rbk02/Desktop/teck-kids/frontend/src/component/HtmlCourse/htmlList.js",["78"],"import React, { useState } from 'react';\nimport * as FaIcons from 'react-icons/fa';\nimport * as AiIcons from 'react-icons/ai';\nimport { Link } from 'react-router-dom';\nimport { SidebarData } from './htmlData';\nimport './htmlList.css';\n\nimport axios from 'axios';\n\n/********************************************************/\nfunction HtmlList() {\n  // false means it's not showing\n  const [ sidebar, setSidebar   ] = useState(false);\n  const [ id ,     setId        ] = useState(\"5fddb8239c7c521c4d2474e1\");\n  const [lesson , setLesson     ] = useState([]);\n  const showSidebar = () => setSidebar(!sidebar);\n  \n  \n  const getLesson = async() => {\n    try {\n    const retrevd = await axios.get(\"http://localhost:8000/course/html/\"+id)  \n    setLesson(retrevd.data)\n    console.log(\"lesson id =\"+id)\n     console.log(lesson)\n     \n    } catch (error) {\n      alert(error)\n    }\n  };\n\n  return (\n    <div className='\"row\"'>\n      \n        <div className='navbar'>\n          <Link to='#' className='menu-bars'>\n            <FaIcons.FaBars onClick={showSidebar} />\n            \n          </Link>\n        </div>\n\n        <nav className={sidebar ? 'nav-menu active' : 'nav-menu'}>\n          <ul className='nav-menu-items' >\n            <li className='navbar-toggle'>\n              <Link to='#' className='menu-bars' onClick={showSidebar}>\n                <AiIcons.AiOutlineClose /><span >HTML Course</span>\n              </Link>\n            </li>\n            {SidebarData.map((item, index) => {\n              return (\n                <li key={index} className={item.cName} onClick={(event)=>{setId(event.currentTarget.dataset.id)}} data-id={item.id} >\n                  <Link to={item.path} >\n                    \n                    <span onClick={getLesson}>{item.title}</span>\n                  </Link>\n                </li>\n              );\n            })}\n          </ul>\n        </nav>\n        <div className=\"courseBox col-lg-8 col-md-6\">\n          <h1>{lesson.Titles}</h1>\n          <p>{lesson.Desceriptions}</p>\n          <iframe width=\"600\" height=\"400\" src={lesson.Videos}frameBorder=\"0\" allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\" allowFullScreen></iframe>\n        </div>  \n                 \n      \n    </div>\n  );\n}\n\nexport default HtmlList;\n","/Users/rbk02/Desktop/teck-kids/frontend/src/component/HtmlCourse/htmlData.js",[],"/Users/rbk02/Desktop/teck-kids/frontend/src/component/HtmlCourse/HTMLcourse.js",[],"/Users/rbk02/Desktop/teck-kids/frontend/src/component/CSSCourse/CSScourse.js",[],"/Users/rbk02/Desktop/teck-kids/fronten